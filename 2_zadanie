
#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;

int fib(int n) {

    int a, b;
    a = 1;
    b = 1;
    int m;
    if (n == 1) {
        return a;
    }
    else if (n == 2) {
        return b;
    }

    else if (n >= 3) {
        for (int i = 2; i < n; i++) {
            m = a+b;
            a = b;
            b = m;
        }

    }
    else {
        return -1;
    }
    return m;
}


int main() {
    setlocale(LC_ALL,"RUSSIAN");

    int n;
    cin>>n;
    cout<<fib(n);


}
