
#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;

int del(int n){
    int s = 0;
    for (int i = 1;i < n; i++){
        if (n % i == 0){
            s += i;
        }
    }
    return s;
}


int main() {
    
    setlocale(LC_ALL,"RUSSIAN");
    for (int i = 1;i <= 10000; i++){
        int x = del(i);
        if (x <= i) continue;
        if (i == del(x) && x <= 10000){
            cout<<i<<" "<<x<<endl;
        }
    }



}
